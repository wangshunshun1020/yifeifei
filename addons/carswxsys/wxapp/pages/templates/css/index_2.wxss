  .swiper-container_2{
    width: 720rpx;
    position: relative;  
    margin-top:20rpx;
  }
  .swiper_box_2 {
    width: 100%;
  
  }
  
  swiper-item_2 image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    border-radius: 20rpx;
  
  }
  .swiper-container_2 .dots_2{  
    position: absolute;  
    left: 0;  
    right: 0;  
    bottom: 20rpx;  
    display: flex;  
    justify-content: center;  
  }  
  .swiper-container_2 .dots_2 .dot_2{  
    margin: 0 8rpx;  
    width: 14rpx;  
    height: 14rpx;  
    background: #fff;  
    border-radius: 50%;  
    transition: all .6s;  
    opacity: .5;
  }   
  .swiper-container_2 .dots_2 .dot_2 .active_2{  
    width: 14rpx;  
    opacity: 1;
  }
  
